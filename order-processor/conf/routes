GET         /v1/orders/:id              com.jotason.phone.v1.orders.controllers.OrderController.get(id: Long)
POST        /v1/orders                  com.jotason.phone.v1.orders.controllers.OrderController.save

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)

